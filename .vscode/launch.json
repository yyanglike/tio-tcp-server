    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "HelloServerStarter",
            "request": "launch",
            "mainClass": "com.example.tio.HelloServerStarter",
            "projectName": "tio-tcp-server"
        },
        {
            "type": "java",
            "name": "TestSharedMenory",
            "request": "launch",
            "mainClass": "com.example.util.TestSharedMenory",
            "vmArgs": " --add-exports=java.base/jdk.internal.ref=ALL-UNNAMED --add-exports=java.base/sun.nio.ch=ALL-UNNAMED --add-exports=jdk.unsupported/sun.misc=ALL-UNNAMED --add-exports=jdk.compiler/com.sun.tools.javac.file=ALL-UNNAMED --add-opens=jdk.compiler/com.sun.tools.javac=ALL-UNNAMED --add-opens=java.base/java.lang=ALL-UNNAMED --add-opens=java.base/java.lang.reflect=ALL-UNNAMED --add-opens=java.base/java.io=ALL-UNNAMED --add-opens=java.base/java.util=ALL-UNNAMED",
            "cwd": "${workspaceFolder}",         
            "projectName": "tio-tcp-server"
        }
    ]
}